

function countBs (string) {
      var count= 0
      
for (var i = 0; i < string.length; i++){
      if (string.charAt(i) === "B") count++
}
return count
}

function countChar (string, x ){
var count= 0

for (var i = 0; i < string.length; i++) {
      if (string.charAt(i) === "x") count++
}
return count
}

console.log(countBs("BANANA"))

